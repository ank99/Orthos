#!/bin/sh

cat working/proper-names.lst                 \
  | comm -23 - r/special/not-possessive      \
  | grep -v "'"                              \
  | src/remove-plurals                       \
  | cat - r/special/add-possessive           \
  | cat - `for d in l/add-possessive/*; do echo $d/*; done` \
  | src/filter                               \
  | sort | uniq > working/possessive-also.lst

